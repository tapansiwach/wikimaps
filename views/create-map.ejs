<!DOCTYPE html>
<html lang="en">

<head>
  <%- include("./partials/_head") %>
</head>

<body id="page-top">
  <div id="wrapper">
    <%- include("./partials/_side_nav"); %>
      <div class="d-flex flex-column" id="content-wrapper">
        <div id="content">
          <%- include("./partials/_header"); %>

            <div class="container-fluid" style="margin: 0px 0px 24px;">



              <h3 class="text-dark mb-4">New Map</h3>
              <div class="card shadow">
                <div class="card-header py-3">
                  <p class="text-primary m-0 fw-bold">Create A Map</p>
                </div>
                <div class="card-body">

                  <form class="new-map">
                    <div class="mb-3"><input class="form-control " type="text" id="newMapTitle" aria-describedby="title"
                        placeholder="Title..." name="title"></div>
                    <div class="mb-3"><input class="form-control " type="text" id="newMapLatitude"
                        aria-describedby="latitude" placeholder="Latitude..." name="latitude"></div>
                    <div class="mb-3"><input class="form-control " type="text" id="newMapLongitude"
                        aria-describedby="longitude" placeholder="Longitude..." name="longitude"></div>

                    <button class="btn btn-primary d-block btn-user w-100" type="submit">Create
                      Map</button>
                    <hr>
                  </form>

                </div>
              </div>




            </div>

        </div>
        <%- include("./partials/_footer"); %>
      </div>
  </div><a class="border rounded d-inline scroll-to-top" href="#page-top"><i class="fas fa-angle-up"></i></a>
  </div>
  </div>
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.0/dist/js/bootstrap.bundle.min.js"></script>
  <script src="assets/js/theme.js"></script>
</body>

</html>
