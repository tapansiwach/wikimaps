                      <table class="table my-0" id="dataTable">
                        <thead>
                          <tr>
                            <th>Owner</th>
                            <th>Map Title</th>
                            <th>City</th>
                            <th><i class="fa fa-heart" style="color: rgb(231,74,58);"></i></th>
                            <th>Created On</th>
                            <th>Updated</th>
                          </tr>
                        </thead>
                        <tbody>
                          <% for (const map of maps) { %>
                            <tr data-mapId="<%= map.id %> ">
                              <td><img class="rounded-circle me-2" width="30" height="30"
                                  src="<%= map.profile_pic_url %>">
                                <%= map.first_name %>
                                  <%= map.last_name %>
                              </td>
                              <td>
                                <%= map.title %>
                              </td>
                              <td>
                                <%= map.city %>
                              </td>
                              <td><i class="fa fa-heart-o"></i></td>
                              <td>
                                <%= map.created_on.getFullYear() + '/' + map.created_on.getMonth() + '/' +
                                  map.created_on.getDate() %>
                              </td>
                              <td>
                                <%= map.modified_on.getFullYear() + '/' + map.modified_on.getMonth() + '/' +
                                  map.modified_on.getDate() %>
                              </td>
                            </tr>
                            <% } %>
